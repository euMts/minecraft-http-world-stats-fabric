{
  "schemaVersion": 1,
  "id": "http_world_stats",
  "version": "${version}",
  "name": "http_world_stats",
  "description": "A Minecraft Fabric mod that exposes world statistics via a local HTTP server.",
  "authors": [],
  "contact": {
    "homepage": "https://github.com/euMts/minecraft-http-world-stats-fabric",
    "sources": "https://github.com/euMts/minecraft-http-world-stats-fabric"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/http_world_stats/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "dev.matheuspass.http_world_stats.client.Http_world_statsDataGenerator"
    ],
    "client": [
      "dev.matheuspass.http_world_stats.client.Http_world_statsClient"
    ],
    "main": [
      "dev.matheuspass.http_world_stats.Http_world_stats"
    ]
  },
  "mixins": [
    "http_world_stats.mixins.json",
    {
      "config": "http_world_stats.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
